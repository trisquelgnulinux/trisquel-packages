        $iflabel{ gpt }
        $reusemethod{ }
        method{ biosgrub } .

538 538 1075 fat32
        $reusemethod{ }
        $primary{ }
        method{ efi }
        format{ } .

1792 1024 2048 ext4
        $defaultignore{ }
        $lvmignore{ }
        method{ format }
        format{ }
        use_filesystem{ }
        filesystem{ ext4 }
        mountpoint{ /boot } .

